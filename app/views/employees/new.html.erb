<h1> Create Employee </h1> 
<%= form_for(@employee) do |f| %> 
    
    <%= f.label :first_name, 'First Name:' %><br>
    <%= f.text_field :first_name %>
    <%= f.label  :last_name, 'Last Name: ' %>
    <%= f.text_field :last_name %>
    <%= f.label :alias, 'Alias:' %> 
    <%= f.text_field :alias %>
    <%= f.label :title, 'Title:' %> 
    <%= f.text_field :title %>
    <%= f.label :office, 'Office:' %> 
    <%= f.text_field :office %>
    <%= f.label :img_url, 'Image_url:' %> 
    <%= f.text_field :img_url %>
    <%= f.collection_select(:dog_id, Dog.all,:id, :name, prompt:true)%>

    

    <div class="actions">
    <%= f.submit %>
  </div>
<%end%> 
    
    
    
